{
    "reporter": [
        "lcov",
        "text",
        "text-summary"
    ],
    "extension": [
        ".ts"
    ],
    "exclude": [
        "**/Mocks",
        "**/Interfaces",
        "**/Constants",
        "**/*.d.ts",
        "**/*.test.ts",
        "**/*.js"
    ],
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "check-coverage": true
}
